<%= form_for [:admin, @trip] do |f| %>
  <%= f.label :duration %>
  <%= f.text_field :duration %>

  <%= f.label :start_date %>
  <%= f.text_field :start_date %>

  <%= f.label :end_date %>
  <%= f.number_field :end_date %>

  <%= f.label :bike_id %>
  <%= f.text_field :bike_id %>

  <%= f.label :subscription_type %>
  <%= f.date_field :subscription_type %>

  <%= f.label :zip_code %>
  <%= f.date_field :zip_code %>

  <%= f.label :start_station_id %>
  <%= f.date_field :start_station_id %>

  <%= f.label :end_station_id %>
  <%= f.date_field :end_station_id %>

  <%= f.submit %>
<% end %>
