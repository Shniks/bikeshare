<nav>
  <ul class="nav-left">
    <div class="brand-logo">
      <i class="fas fa-bicycle"></i>
      <a href="#">Bike Share</a>
    </div>
  </ul>
  <% flash.each do |name, msg| %>
  <%= content_tag :div, msg, class: name %>
  <% end %>

  <ul class="nav-right">
    <li>
      <a href="#">About</a>
    </li>
  </ul>
  <% if current_user %>
    <%= link_to 'Logout', logout_path %>
  <% else %>
    <%= link_to 'Login', login_path, class: 'login' %>
  <% end %>
</nav>
